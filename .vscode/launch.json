{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "AlbumReviewApplication",
      "request": "launch",
      "mainClass": "albumreview.springboot/restserver.AlbumReviewApplication",
      "projectName": "albumreview-springboot"
    },
    {
      "type": "java",
      "name": "LoginPageApp",
      "request": "launch",
      "mainClass": "albumreview.ui/viewutil.LoginPageApp",
      "projectName": "albumreview-ui"
    },
    {
      "type": "java",
      "name": "LogginPageApp",
      "request": "launch",
      "mainClass": "albumreview.ui/viewutil.LogginPageApp",
      "projectName": "albumreview-ui"
    },
    {
      "type": "java",
      "name": "AlbumReviewApp",
      "request": "launch",
      "mainClass": "albumreview.ui/viewutil.AlbumReviewApp",
      "projectName": "albumreview-ui"
    },
    {
      "type": "java",
      "name": "SaveToFile",
      "request": "launch",
      "mainClass": "albumreview.core/json.SaveToFile",
      "projectName": "albumreview-core"
    },
    {
      "type": "java",
      "name": "AlbumReviewModule",
      "request": "launch",
      "mainClass": "albumreview.core/json.AlbumReviewModule",
      "projectName": "albumreview-core"
    },
    {
      "type": "java",
      "name": "AlbumReviewApp",
      "request": "launch",
      "mainClass": "albumreview.ui/ui.AlbumReviewApp",
      "projectName": "albumreview-ui"
    },
    {
      "type": "java",
      "name": "FileHandler",
      "request": "launch",
      "mainClass": "albumreview.core/core.FileHandler",
      "projectName": "albumreview-core"
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch ALBUM-REVIEW - multi-module, multi-package ",
      "request": "launch",
      "mainClass": "albumreview.ui/ui.App",
      "projectName": "albumreview-ui",
      "vmArgs": [
        "--add-opens",
        "albumreview.ui/ui=ALL-UNNAMED",
        "--add-exports",
        "javafx.graphics/com.sun.javafx.application=ALL-UNNAMED"
      ]
    }
  ]
}
